{
  "name": "panel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "glamor": "^2.20.40",
    "glamorous": "^4.11.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "trianglify": "^1.2.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "precommit": "lint-staged",
    "postmerge": "yarnhook",
    "postcheckout": "yarnhook",
    "postrewrite": "yarnhook"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,scss,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.{js,ts,tsx}",
        "options": {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      }
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.35",
    "@types/react-dom": "^16.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "typescript": "^2.6.2",
    "yarnhook": "^0.1.1"
  }
}
